{
  "customize": {
    "build_dir_name": "pencilcc", 
    "env_prefix": "CK_ENV_COMPILER_PENCIL", 
    "languages": [
      "C", 
      "CPP"
    ], 
    "skip_add_to_bin": "no", 
    "skip_add_to_ld_path": "no", 
    "skip_add_to_path": "no", 
    "add_include_path": "yes", 
    "source_to_source": "yes"
  }, 
  "customize_script": "customize", 
  "env": {
    "CK_PPCG": "ppcg", 
    "CK_PENCILCC": "pencilcc",
    "PENCIL_COMPILER_BINARY":"ppcg"
  }, 
  "soft_name": "Compiler - PENCIL", 
  "tags": [
    "source-to-source-compiler", 
    "pencilcc", 
    "polyhedral",
    "lang-c",
    "lang-cpp"
  ], 
  "tool": "pencilcc", 
  "version": {
    "cmd": "--version > $#filename#$ 2>&1", 
    "split": ".", 
    "string_after": "", 
    "string_before": "version "
  }
}

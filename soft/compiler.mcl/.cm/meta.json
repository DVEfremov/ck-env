{
  "customize": {
    "add_win_sdk_path": "yes", 
    "env_prefix": "CK_ENV_COMPILER_MVSC", 
    "input_path_example": "C:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC", 
    "languages": [
      "C", 
      "CPP"
    ], 
    "skip_add_to_bin": "yes", 
    "skip_add_to_ld_path": "yes", 
    "skip_add_to_path": "yes"
  }, 
  "customize_script": "customize", 
  "env": {
    "CK_AR": "lib", 
    "CK_ASM_EXT": ".asm", 
    "CK_CC": "cl", 
    "CK_COMPILER_FLAGS_OBLIGATORY": "/DWINDOWS /GS-", 
    "CK_CXX": "cl", 
    "CK_DLL_EXT": ".dll", 
    "CK_EXE_EXT": ".exe", 
    "CK_FLAGS_CREATE_ASM": "/Fa /c", 
    "CK_FLAGS_CREATE_OBJ": "/c", 
    "CK_FLAGS_DLL": "/MT /DWin /LD /FIwindows.h", 
    "CK_FLAGS_DLL_EXTRA": "/link /dll", 
    "CK_FLAGS_DYNAMIC_BIN": "/MD", 
    "CK_FLAGS_OUTPUT": "/Fe", 
    "CK_FLAGS_STATIC_BIN": "/MT", 
    "CK_FLAGS_STATIC_LIB": "/MT", 
    "CK_FLAG_PREFIX_INCLUDE": "/I", 
    "CK_FLAG_PREFIX_VAR": "/D", 
    "CK_LB": "lib", 
    "CK_LB_OUTPUT": "/OUT:", 
    "CK_LD_DYNAMIC_FLAGS": "/link /NODEFAULTLIB:LIBCMT", 
    "CK_LD_FLAGS_EXTRA": "bufferoverflowU.lib", 
    "CK_LIB_EXT": ".lib", 
    "CK_MAKE": "nmake", 
    "CK_OBJDUMP": "dumpbin /disasm", 
    "CK_OBJ_EXT": ".obj"
  }, 
  "soft_name": "Compiler - Microsoft CL", 
  "tags": [
    "compiler", 
    "mcl",
    "lang-c",
    "lang-cpp"
  ], 
  "tool": "cl", 
  "version": {
    "cmd": "> $#filename#$ 2>&1", 
    "split": ".", 
    "string_after": "for", 
    "string_before": "Version ", 
    "type": "parse_first_string"
  }
}

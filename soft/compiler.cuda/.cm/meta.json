{
  "customize": {
    "can_strip_dynamic_lib": "yes",
    "env_prefix": "CK_ENV_COMPILER_CUDA",
    "input_path_example": "C:\\Program Files\\NVIDIA GPU Computing Toolkit\\CUDA\\v5.5",
    "languages": [
      "C",
      "CPP"
    ],
    "skip_add_to_bin": "no",
    "skip_add_to_ld_path": "no",
    "skip_add_to_lib": "yes",
    "skip_add_to_path": "no"
  },
  "customize_script": "customize",
  "env": {
    "CK_AR": "ar",
    "CK_ASM_EXT": ".s",
    "CK_CC": "nvcc",
    "CK_COMPILER_FLAGS_OBLIGATORY": "",
    "CK_CXX": "nvcc",
    "CK_DLL_EXT": ".so",
    "CK_EXE_EXT": ".out",
    "CK_EXTRA_LIB_DL": "-ldl",
    "CK_EXTRA_LIB_M": "-lm",
    "CK_FLAGS_CREATE_ASM": "-S",
    "CK_FLAGS_CREATE_OBJ": "-c",
    "CK_FLAGS_DLL": "-shared -fPIC",
    "CK_FLAGS_DLL_EXTRA": "",
    "CK_FLAGS_DYNAMIC_BIN": "",
    "CK_FLAGS_OUTPUT": "-o ",
    "CK_FLAGS_STATIC_BIN": "-static -fPIC",
    "CK_FLAGS_STATIC_LIB": "-fPIC",
    "CK_FLAG_PREFIX_INCLUDE": "-I",
    "CK_FLAG_PREFIX_LIB_DIR": "-L",
    "CK_FLAG_PREFIX_VAR": "-D",
    "CK_LB": "ar rcs",
    "CK_LB_OUTPUT": "-o ",
    "CK_LD_FLAGS_EXTRA": "",
    "CK_LIB_EXT": ".a",
    "CK_MAKE": "make.exe",
    "CK_OBJDUMP": "cuobjdump --dump-sass",
    "CK_OBJ_EXT": ".o",
    "CK_PLUGIN_FLAG": "-fplugin=",
    "CK_PROFILER": "gprof"
  },
  "soft_name": "Compiler - NVIDIA CUDA",
  "tags": [
    "compiler",
    "cuda",
    "lang-c-cuda",
    "lang-cpp-cuda"
  ],
  "tool": "nvcc",
  "version": {
    "cmd": "--version > $#filename#$ 2>&1",
    "split": ".",
    "string_after": "",
    "string_before": ", V"
  }
}

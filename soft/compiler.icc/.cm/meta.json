{
  "customize": {
    "build_dir_name": "icc", 
    "env_prefix": "CK_ENV_COMPILER_ICC", 
    "languages": [
      "C", 
      "CPP", 
      "F77", 
      "F90", 
      "F95"
    ], 
    "skip_add_to_bin": "yes", 
    "skip_add_to_ld_path": "yes", 
    "skip_add_to_path": "yes"
  }, 
  "customize_script": "customize", 
  "env": {
    "CK_AR": "ar", 
    "CK_ASM_EXT": ".s", 
    "CK_CC": "icc", 
    "CK_COMPILER_FLAGS_OBLIGATORY": "", 
    "CK_CXX": "icc", 
    "CK_DLL_EXT": ".so", 
    "CK_EXE_EXT": ".out", 
    "CK_F90": "ifort", 
    "CK_F95": "ifort", 
    "CK_FC": "ifort", 
    "CK_FLAGS_CREATE_ASM": "-S", 
    "CK_FLAGS_CREATE_OBJ": "-c", 
    "CK_FLAGS_DLL": "-shared -fPIC", 
    "CK_FLAGS_DLL_EXTRA": "", 
    "CK_FLAGS_OUTPUT": "-o ", 
    "CK_FLAGS_STATIC_BIN": "-static -fPIC", 
    "CK_FLAGS_STATIC_LIB": "-fPIC", 
    "CK_FLAG_PREFIX_INCLUDE": "-I", 
    "CK_FLAG_PREFIX_VAR": "-D", 
    "CK_LB": "ar rcs", 
    "CK_LB_OUTPUT": "-o ", 
    "CK_LD_FLAGS_EXTRA": "", 
    "CK_LIB_EXT": ".a", 
    "CK_MAKE": "make", 
    "CK_OBJDUMP": "objdump -d", 
    "CK_OBJ_EXT": ".o", 
    "CK_PLUGIN_FLAG": "-fplugin=", 
    "CK_PROFILER": "gprof",
    "CK_EXTRA_LIB_M":"-lm",
    "CK_EXTRA_LIB_DL":"-ldl"
  }, 
  "soft_name": "Compiler - Intel", 
  "tags": [
    "compiler", 
    "icc", 
    "lang-c", 
    "lang-cpp", 
    "lang-f77", 
    "lang-f90", 
    "lang-f95"
  ], 
  "tool": "icc", 
  "version": {
    "cmd": "--version > $#filename#$ 2>&1", 
    "split": ".", 
    "string_after": " ", 
    "string_before": ") "
  }
}
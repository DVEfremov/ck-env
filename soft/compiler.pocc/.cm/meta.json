{
  "tags": [
    "compiler", 
    "source-to-source",
    "pocc",
    "lang-c",
    "lang-f77"
  ], 
  "customize": {
    "languages": [
      "C", 
      "F77"
    ], 
    "source_to_source":"yes",
    "skip_add_to_ld_path": "yes", 
    "skip_add_to_bin": "yes", 
    "env_prefix": "CK_ENV_COMPILER_POCC", 
    "skip_add_to_path": "yes"
  }, 
  "tool": "pocc", 
  "soft_name": "Compiler - POCC", 
  "version": {
    "type": "parse_first_string", 
    "string_before": "version ", 
    "cmd": "--version > $#filename#$ 2>&1", 
    "string_after": "", 
    "split": "."
  }, 
  "customize_script": "customize", 
  "env": {
    "CK_COMPILER_FLAGS_OBLIGATORY": "", 
    "CK_AR": "$#tool_prefix#$ar", 
    "CK_FLAGS_DLL_EXTRA": "-ldl", 
    "CK_LIB_EXT": ".a", 
    "CK_CC": "$#tool_prefix#$pocc", 
    "CK_LB_OUTPUT": "-o ", 
    "CK_FLAGS_STATIC_LIB": "-fPIC", 
    "CK_FLAG_PREFIX_VAR": "-D", 
    "CK_LD_FLAGS_EXTRA": "-lm -ldl", 
    "CK_FLAGS_CREATE_OBJ": "-c", 
    "CK_OBJ_EXT": ".o", 
    "CK_FLAGS_OUTPUT": "-o", 
    "CK_LB": "$#tool_prefix#$ar rcs", 
    "CK_MAKE": "make", 
    "CK_DLL_EXT": ".so", 
    "CK_ASM_EXT": ".s", 
    "CK_FLAG_PREFIX_INCLUDE": "-I", 
    "CK_EXE_EXT": ".out", 
    "CK_PROFILER": "gprof", 
    "CK_FLAGS_DLL": "-shared -fPIC", 
    "CK_FLAGS_STATIC_BIN": "-static -fPIC", 
    "CK_FLAGS_CREATE_ASM": "-S"
  }
}
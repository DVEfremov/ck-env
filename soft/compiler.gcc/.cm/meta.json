{
  "auto_detect": "yes", 
  "customize": {
    "add_only_with_version": "yes", 
    "add_to_path": "yes", 
    "build_dir_name": "gcc", 
    "can_strip_dynamic_lib": "yes", 
    "search_numeric_ext_on_linux": "yes",
    "check_that_exists": "yes", 
    "ck_version": 10, 
    "env_prefix": "CK_ENV_COMPILER_GCC", 
    "languages": [
      "C", 
      "CPP", 
      "F77", 
      "F90", 
      "F95"
    ], 
    "limit_recursion_dir_search": {"linux":3, "win":4}, 
    "only_for_target_os_tags": [
      "mingw", 
      "linux"
    ], 
    "soft_file": {
      "linux": "gcc", 
      "win": "gcc.exe"
    }, 
    "soft_path_example": {
      "linux": "/usr/bin/gcc", 
      "win": "C:\\Program Files\\GCC-4.9.2-x64\\bin\\gcc.exe"
    }, 
    "soft_version_cmd": {
      "linux": "--version > $#filename#$ 2>&1", 
      "win": " --version > $#filename#$ 2>&1"
    }
  }, 
  "soft_name": "GCC compiler", 
  "tags": [
    "compiler", 
    "gcc", 
    "lang-c", 
    "lang-cpp", 
    "lang-f77", 
    "lang-f90", 
    "lang-f95"
  ]
}

    "skip_add_to_ld_path": "yes"
  }, 
  "env": {
    "CK_JAVAC": "javac", 
    "CK_JAVA_OBJ_EXT": ".class", 
    "CK_JAVA_RT": "java"
  }, 
  "soft_name": "Compiler - JAVA", 

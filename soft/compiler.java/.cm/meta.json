{
  "customize": {
    "build_dir_name": "java", 
    "env_prefix": "CK_ENV_COMPILER_JAVA", 
    "languages": [
      "JAVA"
    ], 
    "skip_add_to_ld_path": "yes"
  }, 
  "env": {
    "CK_COMPILER_FLAGS_OBLIGATORY": "", 
    "CK_FLAG_PREFIX_INCLUDE": "-classpath ", 
    "CK_JAVAC": "javac", 
    "CK_JAVA_RT": "java -classpath .", 
    "CK_OBJ_EXT": ".class"
  }, 
  "soft_name": "Compiler - JAVA", 
  "tags": [
    "compiler", 
    "java", 
    "lang-java"
  ], 
  "tool": "javac", 
  "version": {
    "cmd": "-version > $#filename#$ 2>&1", 
    "split": ".", 
    "string_after": "", 
    "string_before": "javac "
  }
}
